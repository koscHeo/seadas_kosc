get the latest software from
ftp://ftp.gnu.org/gnu/gsl/
current version: 1.16

setenv CFLAGS -m64
setenv F77 f77
setenv FFLAGS "-m64 -extend_source -convert big_endian -assume byterecl -save -O3"


tar xzf gsl-<version>.tar.gz
cd gsl-<version>
./configure --prefix=/disk01/home/swdev/ocssw/lib3_64/gsl --disable-shared
make
make check
make install
cd ..
rm -rf gsl-<version>

